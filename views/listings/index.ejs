<% layout('layouts/boilerplate') -%>
<body>
    <h3>All Listings</h3>
    <form method="Get" action="/listings/new">
        <button>Add new list</button>
    </form>
    <ul>
        <% for(let listing of allListings) { %>
            <li>
                <a href="/listings/<%= listing._id %>"><%= listing.title %></a>
            </li>
        <% } %>
    </ul>
</body>
